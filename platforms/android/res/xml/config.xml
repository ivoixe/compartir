<?xml version='1.0' encoding='utf-8'?>
<widget id="com.prisma.share" version="0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <name>Proyecto compartir</name>
    <description>
       PROYECTO DE PRUEBAS PARA EL PLUGIN DE COMPARTIR
    </description>
    <icon src="icon.png" />
    <author email="ivonne@prismacm.com" href="http://prismacm.com">
        Prisma Cm
    </author>
    <content src="index.html" />
    <access origin="*" />
    <config-file parent="/manifest" target="AndroidManifest.xml">
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    </config-file>
</widget>
